{
  "tags": {
    "dictionaries": [ "closure", "jsdoc" ]
  },
  "source": {
    "include": [
      "lib",
      "externs/shaka"
    ]
  },
  "templates": {
    "default": {
      "sortTutorialsByConfigOrder": true,
      "useLongnameInNav": true,
      "navOrder": [
        "Tutorials",
        "Classes",
        "Interfaces",
        "Events"
      ]
    }
  },
  "plugins": [ "docs/jsdoc-plugin" ],
  "opts": {
    "private": true,
    "recurse": true,
    "tutorials": "docs/tutorials",
    "template": "docs/jsdoc-template",
    "destination": "docs/api"
  }
}
